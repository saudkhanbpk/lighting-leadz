{
  "name": "Lighting Leadz",
  "description": "Lighting Leadz chrome extension to automate the process of indeed messages",
  "version": "1.0.0",
  "manifest_version": 3,
  "incognito": "split",
  "permissions": [
    "storage",
    "alarms",
    "tabs",
    "scripting",
    "activeTab"  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "Lighting Leadz",
    "default_popup": "index.html"
}
}